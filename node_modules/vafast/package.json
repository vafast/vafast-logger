{
  "name": "vafast",
  "version": "0.3.9",
  "description": "极简结构化Web框架，支持 Bun 和 Node.js。Go风格，函数优先。",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vafast/vafast.git"
  },
  "homepage": "https://github.com/vafast/vafast#readme",
  "bugs": {
    "url": "https://github.com/vafast/vafast/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./mod": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./node-server": {
      "types": "./dist/node-server/index.d.ts",
      "import": "./dist/node-server/index.js",
      "require": "./dist/node-server/index.js",
      "default": "./dist/node-server/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup",
    "dev": "bun --watch example/index.ts",
    "start": "bun run dist/index.js",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit/",
    "test:integration": "vitest run tests/integration/",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:types": "tsc --noEmit",
    "benchmark": "bun run benchmarks/micro/router.bench.ts",
    "lint": "eslint src tests benchmarks",
    "lint:fix": "eslint src tests benchmarks --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"benchmarks/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"benchmarks/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run test",
    "release:patch": "npm version patch && npm run build && npm run test && npm publish --access=public && git push && git push --tags",
    "release:minor": "npm version minor && npm run build && npm run test && npm publish --access=public && git push && git push --tags",
    "release:major": "npm version major && npm run build && npm run test && npm publish --access=public && git push && git push --tags"
  },
  "keywords": [
    "bun",
    "node",
    "framework",
    "minimal",
    "router",
    "go",
    "web",
    "edge",
    "typescript",
    "fast"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "Vafast Team",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/cookie": "^1.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.30",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitest/ui": "^3.2.4",
    "bun-types": "^1.2.20",
    "eslint": "^9.24.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.39",
    "@vue/server-renderer": "^3.5.18",
    "bun": "^1.2.15",
    "cookie": "^1.0.2",
    "qs": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vue": "^3.5.18"
  }
}
