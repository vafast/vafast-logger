{
  "name": "@vafast/logger",
  "version": "0.2.9",
  "description": "High-performance logging plugin for Vafast framework based on Pino",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "vafast",
    "logger",
    "pino",
    "logging",
    "typescript"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "npx tsx watch example/index.ts",
    "test": "npx vitest run",
    "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "rimraf": "^6.0.1",
    "tsdown": "^0.19.0-beta.4",
    "typescript": "^5.4.5",
    "vafast": "^0.5.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "vafast": "^0.5.1"
  }
}
